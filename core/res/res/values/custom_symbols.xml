<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Automatic brightness enhancements -->
    <java-symbol type="integer" name="config_autoBrightnessBrighteningLightFastDebounce"/>

    <!-- Button Brightness -->
    <java-symbol type="integer" name="config_buttonBrightnessSettingMinimum" />
    <java-symbol type="integer" name="config_buttonBrightnessSettingMaximum" />
    <java-symbol type="integer" name="config_buttonBrightnessSettingDefault" /> 
    
    <!-- Package Manager -->
    <java-symbol type="array" name="config_disabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- sRGB mode -->
    <java-symbol type="string" name="config_srgb_path" />

    <!-- Whether device has dash charging support -->
    <java-symbol type="bool" name="config_hasDashCharger" />

    <!-- Whether notify fingerprint client of successful cancelled authentication -->
    <java-symbol type="bool" name="config_notifyClientOnFingerprintCancelSuccess" />

    <!-- config softap extention feature -->
    <java-symbol type="bool" name="config_softap_extention" />

    <!-- Notifications -->
    <java-symbol type="drawable" name="ic_audio_notification_new" />
    <java-symbol type="drawable" name="ic_audio_notification_mute_new" />
    <java-symbol type="drawable" name="ic_audio_notification_mute_alpha" />
    
    <!-- Config Power Menu -->
    <java-symbol type="drawable" name="ic_lock_reboot" />
    <java-symbol type="string" name="global_action_reboot" />
    <java-symbol type="string" name="reboot_reboot" />
    <java-symbol type="array" name="shutdown_reboot_actions" />
    <java-symbol type="array" name="shutdown_reboot_options" />
    <java-symbol type="string" name="reboot_system" />
    <java-symbol type="string" name="reboot_progress" />

    <java-symbol type="drawable" name="ic_lock_power_screenshot" />
    <java-symbol type="string" name="powermenu_screenshot" />
    <java-symbol type="drawable" name="ic_settings_powermenu" />

    <!-- Screen Recording -->
    <java-symbol type="bool" name="config_enableScreenrecordChord" />

    <java-symbol type="drawable" name="ic_lock_power_screenrecord" />
    <java-symbol type="string" name="powermenu_screenrecord" />

    <!-- Powermenu Torch -->
    <java-symbol type="drawable" name="ic_lock_torch" />
    <java-symbol type="string" name="global_action_torch" />

    <!-- Kill app long press back -->
    <java-symbol type="string" name="app_killed_message" />
    <java-symbol type="string" name="no_app_killed_message" />
    <java-symbol type="integer" name="config_backKillTimeout" />

</resources>
